{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/**/*.ts", "src/preload/*.d.ts", "src/shared/**/*", "src/main/app-config/default-config.ts"],
  "compilerOptions": {
    "composite": true,
      "baseUrl": "./",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"],
      "@root/*": ["src/*"]
    } 
  }
}
